<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	
    <Frame name="Vendor_Dialog_Template2" virtual="true" enableMouse="true" frameStrata="HIGH" movable="true" toplevel="true" inherits="PortraitFrameTemplate">
    </Frame>

    <Frame virtual="true" name="Vendor_BorderFrame" inherits="TooltipBackdropTemplate">
        <KeyValues>
            <KeyValue key="backdropBorderColor" value="LIGHTGRAY_FONT_COLOR" type="global"/>
            <KeyValue key="backdropColor" value="LIGHTGRAY_FONT_COLOR" type="global"/>
            <KeyValue key="backdropColorAlpha" value="0.25" type="number"/>
        </KeyValues>
        <Frames>
            <Frame>
                <Scripts>
                    <OnLoad>self:GetParent():TooltipBackdropOnLoad()</OnLoad>
                </Scripts>
            </Frame>
        </Frames>
    </Frame>
    
</Ui>
